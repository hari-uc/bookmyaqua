let PasswordOtpPage = class PasswordOtpPage {
    constructor(nav, menu, api, util) {
        this.nav = nav;
        this.menu = menu;
        this.api = api;
        this.util = util;
        this.otp = {};
        this.err = {};
        this.menu.enable(false);
    }
    ngOnInit() {
        setTimeout(() => {
            this.a.setFocus();
        }, 150);
    }
    moveFocus(event, nextElement, previousElement) {
        if (event.keyCode == 8 && previousElement) {
            previousElement.setFocus();
        }
        else if (event.keyCode >= 48 && event.keyCode <= 57) {
            if (nextElement) {
                nextElement.setFocus();
            }
        }
        else {
            event.path[0].value = "";
        }
    }
    sendOtp() {
        this.otp = this.otp.a + this.otp.b + this.otp.c + this.otp.d;
        let data = {
            otp: this.otp,
            phone_no: this.api.phone_no,
            type: 0
        };
        this.util.presentLoading();
        this.api.postData('forgot/validate', data).subscribe((data) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            yield this.util.dismissLoading();
            if (data.success) {
                localStorage.setItem('otpToken', data.data.token);
                localStorage.setItem('token', data.data.token);
            }
            this.nav.navigateForward("/change-password");
        }), err => {
            this.err = err.error.errors || err.error.message;
            this.util.presentToast(this.err);
        });
    }
};

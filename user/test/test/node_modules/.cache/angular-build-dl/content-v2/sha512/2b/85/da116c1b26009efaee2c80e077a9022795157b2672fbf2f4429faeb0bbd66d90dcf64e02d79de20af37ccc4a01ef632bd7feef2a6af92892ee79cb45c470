pi.service\";\nimport { UtilService } from \"src/app/services/util.service\";\n@Component({\n  selector: \"app-signup\",\n  templateUrl: \"./signup.page.html\",\n  styleUrls: [\"./signup.page.scss\"],\n})\nexport class SignupPage implements OnInit {\n  @ViewChild(\"input\", { static: true }) myInput;\n  name: any = \"\";\n  email: any = \"\";\n  password: any = \"\";\n  confirmPass: any = \"\";\n  err: any;\n  showPassword: boolean = false;\n  constructor(\n    private nav: NavController,\n    private menu: MenuController,\n    private api: ApiService,\n    private util: UtilService\n  ) {\n    this.menu.enable(false);\n  }\n\n  ngOnInit() {}\n  signIn() {\n    this.util.presentLoading();\n    let data = {\n      name: this.name,\n      email: this.email,\n      password: this.password,\n      phone_no: this.confirmPass,\n    };\n    this.api.postData(\"register\", data).subscribe(\n      (success: any) => {\n        if (success.success) {\n          localStorage.setItem(\"token\", success.data.token);\n          this.api.profileUpdate.next(true);\n          this.nav.navigateRoot(\"home\");\n          this.util.dismissLoading();\n        } else {\n          this.util.dismissLoading();\n        }\n      },\n      (err) => {\n        this.err = err.error.errors;\n        this.util.dismissLoading();\n      }\n    );\n  }\n\n  login(){\n    this.nav.navigateForward('signin');\n  }\n}\n","export default \"ion-content {\\n  --background: url('LogIn.png') no-repeat center center / cover;\\n}\\nion-content .logo_div {\\n  padding-top: 16px;\\n  text-align: center;\\n}\\nion-content .logo_div .logo_tittle {\\n  font-size: 40px;\\n  color: var(--white);\\n  font-family: \\\"HornbillPersonalUse_b\\\";\\n  margin-bottom: 0;\\n}\\nion-content .logo_div .page_tittle {\\n  font-size: 18px;\\n  color: var(--white);\\n  font-family: \\\"rubik_r\\\";\\n  margin: 0;\\n}\\nion-content .logo_div .main_lbl {\\n  font-size: 30px;\\n  font-family: \\\"rubik_md\\\";\\n  color: var(--white);
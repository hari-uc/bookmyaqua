/* harmony import */ var src_app_services_util_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! src/app/services/util.service */ "2Rin");








let ForgotPage = class ForgotPage {
    constructor(nav, menu, api, util) {
        this.nav = nav;
        this.menu = menu;
        this.api = api;
        this.util = util;
        this.err = {};
        this.menu.enable(false);
    }
    ngOnInit() {
        setTimeout(() => {
            this.myInput.setFocus();
        }, 150);
    }
    verify() {
        let data = {
            phone_no: this.phone_no,
            type: 0
        };
        this.util.presentLoading();
        this.api.postData('forgot', data).subscribe((data) => {
            this.util.dismissLoading();
            this.api.phone_no = this.phone_no;
            if (data.success) {
                this.nav.navigateForward("/password-otp");
            }
            else {
                this.util.presentToast(data.msg);
            }
        }, err => {
            this.err = err.error.errors || err.error.message;
            this.util.presentToast(this.err);
        });
    }
};
ForgotPage.ctorParameters = () => [
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_4__["NavController"] },
